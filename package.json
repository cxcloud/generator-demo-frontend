{
  "name": "cxcloud-demo",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "serve dist/ --single",
    "build": "ng build --prod --build-optimizer --extract-css --env=${ENVIRONMENT:-prod} && npm run generate-config",
    "generate-config": "ts-node scripts/extract-config.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && now && now alias",
    "format": "prettier --write 'src/**/*.{css,scss,js,ts,json}'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{css,scss,js,ts,json}'": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/common": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "angular2-image-zoom": "^1.2.1",
    "autocomplete.js": "^0.29.0",
    "bulma": "^0.6.1",
    "bulma-badge": "0.0.3",
    "core-js": "^2.5.7",
    "handlebars": "^4.0.12",
    "mdi": "^2.2.43",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.5",
    "@angular/cli": "6.2.5",
    "@angular/compiler": "6.1.10",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@cxcloud/ct-types": "^0.1.6",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.12.0",
    "codelyzer": "^4.5.0",
    "husky": "^0.14.3",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "lint-staged": "^4.3.0",
    "mkdirp": "^0.5.1",
    "ngx-markdown": "^1.6.0",
    "ngx-webstorage": "^2.0.0",
    "prettier": "^1.14.3",
    "protractor": "^5.4.1",
    "rxjs-tslint": "^0.1.5",
    "serve": "^6.5.8",
    "ts-node": "~3.2.0",
    "tslint": "^5.11.0",
    "typescript": "2.9.2"
  }
}
